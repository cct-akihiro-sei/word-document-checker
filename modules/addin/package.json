{
  "name": "document-checker-addin",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/cct-akihiro-sei/word-document-checker.git"
  },
  "license": "Unlicense",
  "config": {
    "app_to_debug": "word",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "postinstall": "ts-node postinstall.ts",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "start": "office-addin-debugging start manifests/manifest-local.xml",
    "start:desktop": "office-addin-debugging start manifests/manifest-local.xml desktop",
    "start:web": "office-addin-debugging start manifests/manifest-local.xml web",
    "stop": "office-addin-debugging stop manifests/manifest-local.xml",
    "validate": "office-addin-manifest validate manifests/manifest-local.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@fluentui/react": "^8.52.3",
    "core-js": "^3.9.1",
    "es6-promise": "^4.2.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "regenerator-runtime": "^0.13.7",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/office-js": "^1.0.256",
    "@types/office-runtime": "^1.0.23",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-hot-loader": "^4.1.1",
    "@types/webpack": "^4.4.34",
    "acorn": "^8.7.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^8.18.0",
    "eslint-plugin-office-addins": "^2.0.3",
    "eslint-plugin-react": "^7.30.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "less": "^3.9.0",
    "less-loader": "^10.0.1",
    "office-addin-cli": "^1.3.8",
    "office-addin-debugging": "^4.3.14",
    "office-addin-dev-certs": "^1.7.10",
    "office-addin-lint": "^2.0.3",
    "office-addin-manifest": "^1.8.1",
    "office-addin-prettier-config": "^1.1.7",
    "office-addin-validator": "^1.0.5",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.8.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "4.7.4"
  },
  "prettier": "office-addin-prettier-config"
}
